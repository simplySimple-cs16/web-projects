<%-include("partials/header");-%>
<h1>Home</h1>
<p><%=startingContent%></p>

<%posts.forEach(function(post){ %>
    <h1><%=post.title%></h1>
    <%let result = post.content.slice(0,100)+(post.content.length > 100 ? "..." : "");%>
    <p>
      <%=result%>
      <a href="/posts/<%=post.title%>">Read More</a>
    </p>


<%});%>

<%-include("partials/footer");-%>
